<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/start_scan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="18dp"
        android:text="start scan"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/finish_scan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="18dp"
        android:text="finish scan"
        app:layout_constraintStart_toEndOf="@+id/start_scan"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.123" />

    <ListView
        android:id="@+id/result_list"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="155dp"
        app:layout_constraintBottom_toTopOf="@+id/stamp_D"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckedTextView
        android:id="@+id/stamp_A"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="A"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/stamp_C" />

    <CheckedTextView
        android:id="@+id/stamp_B"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="B"
        android:textSize="30dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/stamp_C"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckedTextView
        android:id="@+id/stamp_C"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="300dp"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="C"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/result_list" />

    <CheckedTextView
        android:id="@+id/stamp_D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="D"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/stamp_A"
        app:layout_constraintStart_toStartOf="@+id/stamp_A"
        app:layout_constraintEnd_toEndOf="@+id/stamp_A" />

    <CheckedTextView
        android:id="@+id/stamp_E"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="E"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/stamp_B"
        app:layout_constraintStart_toStartOf="@+id/stamp_B"
        app:layout_constraintEnd_toEndOf="@+id/stamp_B" />

    <CheckedTextView
        android:id="@+id/stamp_F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="F"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/stamp_C"
        app:layout_constraintStart_toStartOf="@+id/stamp_C"
        app:layout_constraintEnd_toEndOf="@+id/stamp_C" />

    <CheckedTextView
        android:id="@+id/stamp_G"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="G"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/stamp_D"
        app:layout_constraintStart_toStartOf="@+id/stamp_D"
        app:layout_constraintEnd_toEndOf="@+id/stamp_D" />

    <CheckedTextView
        android:id="@+id/stamp_H"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="H"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/stamp_E"
        app:layout_constraintStart_toStartOf="@+id/stamp_E"
        app:layout_constraintEnd_toEndOf="@+id/stamp_E" />

    <CheckedTextView
        android:id="@+id/stamp_I"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:checkMark="?android:attr/listChoiceIndicatorMultiple"
        android:text="I"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/stamp_F"
        app:layout_constraintStart_toStartOf="@+id/stamp_F"
        app:layout_constraintEnd_toEndOf="@+id/stamp_F" />

</androidx.constraintlayout.widget.ConstraintLayout>